.yuding{
  width: 100%;
  height: 130px;
  margin: 20px 0;
  padding-left: 5%;
  color:#9D9D9D;
  margin-bottom: 30px;
}
.yuding .yud_box{
  width: 28%;
  height: 100%;
  border: 2px solid #388dd5;
  border-radius: 10px;
  float: left;
  margin-right: 5%;
  text-align: center;
  padding-top: 12px;
  line-height: 30px;
  box-shadow: -3rpx	-4rpx 14rpx 3rpx rgba(170, 169, 169, 0.3);
  position: relative;
  transition: 0.6s;
}
.yuding .yud_box text{
  z-index: 100;
}
.icon_image{
  width: 42px;
  height: 36px;
  position: absolute;
  left: 0px;
  bottom: -4px;
  border-radius: 10px;
  background-image: url(http://m.qpic.cn/psc?/V519zvyD1C0EL63KyDum1jWkcI0Igc3R/ruAMsa53pVQWN7FLK88i5lyjWiNw196UNuXxo9mvndGLGBnu2v2Wa1VQZD3xsfqr7izuXY55k2faL*tfNa4K3bTur3OySgu2ArL0ZOQYzew!/b&bo=pAWwBIUHQAYDB*M!&rf=viewer_4);
  background-size: 100% 100%;
  /* background-color: #fff; */
  z-index: 1;
}

