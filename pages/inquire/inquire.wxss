/*  */
.yuding_bup{
  width: 100%;
  height: 70vh;
  background: linear-gradient(to right,#6FABDD,#6FABDD);
  /* background-color: #fff; */

  border-radius: 20px 20px 0 0;
  overflow: hidden;
  box-shadow: -4rpx	-4rpx 14rpx 3rpx rgba(206, 206, 206, 0.3);
}
.yuding_bup .framework{
  width: 90%;
  height: 50px;
  margin: 0 auto;
  margin-top: 30px;
  line-height: 50px;
}
.yuding_bup .framework select{
  margin-left: 10px;
  width: 72%;
  height: 100%;
  float: right;
  background-color: rgb(255, 255, 255);
  border-radius: 10px;
  box-shadow: 3rpx	4rpx 14rpx 3rpx rgba(86 , 117, 179, 0.3);
  position: relative;
}
.nav_oncik{
  transform: scale(0.95, 0.95);
}


.yuding_bup .framework text{
  margin-left: 5%;

}


.yuding_bup  .but_yuy{
  width: 90%;
  height: 100px;
  margin: 40px auto;
}
.yuding_bup  .but_yuy button{
  width: 40%;
  height: 41px;
  border-radius: 20px;
  background:#FFFFFF;
  color: #224572;
  border: 1px solid#d0d0d0;
  font-size: 16px;
  box-shadow: 3rpx	4rpx 14rpx 3rpx rgba(86 , 117, 179, 0.3);
}





/* 下拉框 */
.top-selected{
  width: 100%;
  height: 100%;
}
.top-selected .iconfont{
  float: right;
  margin-right: 30px;
  
}
.select_box {
  background: #fff;
  width: 100%;
  height: 105px;
  line-height: 35px;
  text-align: center;
  z-index: 111;
  position: absolute;
  top: 100%;
  left: 0;
  border-radius: 10px;
  box-shadow: 3rpx	6rpx 8rpx 4rpx rgba(91, 102, 209, 0.3);
}


.select_box .select_one{
  font-size: 28rpx;
  flex: 1;
  /* color: #ffffff; */
  border-radius: 10px;
  border-top: 1px solid  #eeeeee;
  
}
