/* pages/only/only.wxss */
.only_box{
  width: 90%;
  margin: 0 auto;
  padding: 20px 5px;
  overflow: hidden;
}
.only_box h2{
  line-height: 40px;
  font-size: 17px;
  font-weight: 600;
}
.only_box .phone{
  width: 100%;
  height: 40px;
  /* background-color: aqua; */
  line-height: 40px;
}
.only_box  text{
  width: 9%;
  float: left;
  margin: 0px 3%;
}
.only_box input{
  width: 80%;
  height: 40px;
  float: left;
  background-color: #EDEDED;
  color: #999999;
  border-radius: 20px;
  padding-left: 5%;
  margin-bottom: 10px;
}
.only_box .inp_sr{
  width: 95%;
}
.yz_position{
  width: 100%;
  height: 40px;
  position: relative;
}
.yz_position navigator{
  position: absolute;
  top: 25%;
  right: 6%;
  color: #346DE0;
  z-index: 88;
}

